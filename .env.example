# Server Configuration
PORT=3000
NODE_ENV=development

# Database Configuration
# Local PostgreSQL: postgresql://username:password@localhost:5432/database_name
# Render: This will be automatically provided as DATABASE_URL environment variable
DATABASE_URL=postgresql://localhost:5432/financeu

# JWT Secret - MUST be changed in production!
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Session Settings
SESSION_EXPIRY=7d
